{
  "description": "Demonstrating webRequests",
  "manifest_version": 2,
  "name": "Block webpage",
  "version": "1.0",

  "icons": {
    "60": "icons/icon-60.png"
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["background.js"]
  }
}
